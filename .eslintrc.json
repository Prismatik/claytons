{
  "extends": "prismatik",
  "rules": {
    "arrow-body-style": "off",
    "camelcase": "off",
    "global-require": "off",
    "import/no-unresolved": "off",
    "max-len": "off",
    "new-cap": "off",
    "no-console": "off",
    "no-param-reassign": "off",
    "spaced-comment": "off",
    "no-underscore-dangle": "off",
    "func-names": "off"
  }
}
